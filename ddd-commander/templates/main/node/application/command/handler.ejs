import { CommandHandler, ICommandHandler } from '@nestjs/cqrs';

@CommandHandler(<%= classCommand %>)
export class <%= className %>
implements ICommandHandler<<%= classCommand %>> {
  constructor(private service: <%= classService %>) {}

  async execute(command: <%= classCommand %>): Promise<void> {
    const reference = command.reference ? command.reference : '';
    await this.service.createClient(
    command.id,
    command.name,
    command.address,
    reference,
    command.lat,
    command.long,
    command.cellphone,
    command.storeId,
    );
  }
}
