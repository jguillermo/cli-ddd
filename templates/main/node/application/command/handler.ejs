import { CommandHandler, ICommandHandler } from '@nestjs/cqrs';

@CommandHandler(<%= classCommand %>)
export class <%= className %> implements ICommandHandler<<%= classCommand %>> {
  constructor(private service: <%= classService %>) {}

  async execute(command: <%= classCommand %>): Promise<void> {
  <%_ properties.forEach(function(propertie){ _%>
    const <%= propertie.name.value %> = new <%= propertie.className %>(command.<%= propertie.name.value %>);
  <%_ }); _%>

    await this.service.execute(<%= strProperties %>);
  }
}
