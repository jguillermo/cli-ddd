import { StringType } from 'base-ddd';

<%_ if (propertie.propertie.required.value) { _%>
export class <%= className %> extends StringType {
  isValid(): boolean {
    if (this.isNull) {
      throw new Error('is required.');
    }
    if (this.isEmpty) {
      throw new Error('should not be empty.');
    }
    return true;
  }
}
<%_ }else{ _%>
export class <%= className %> extends StringType {}
<%_ } _%>
